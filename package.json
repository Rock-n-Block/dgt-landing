{
  "name": "dgt-landing",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "formik": "^2.2.9",
    "rc-checkbox": "^2.3.2",
    "rc-dialog": "^8.7.0",
    "rc-scrollbars": "^1.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.8",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^8.2.0",
    "typescript": "^4.1.2",
    "use-debounce": "^7.0.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.0",
    "@types/node": "^16.11.7",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "node-sass": "^6.0.1",
    "plop": "^3.0.5",
    "prettier": "^2.4.1",
    "react-error-overlay": "^6.0.10",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack": "^5.72.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"{src,types,scripts}/**/*.{json,{j,t}s{,x},scss,}\"",
    "lint": "npm run lint:ts",
    "lint:ts": "eslint \"{src,types}/**/*.ts{,x}\"",
    "lint-fix": "npm run format && npm run lint-fix:ts",
    "lint-fix:ts": "eslint \"{src,types}/**/*.ts{,x}\" --fix",
    "new": "plop --plopfile generators/index.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  }
}
